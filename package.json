{
  "name": "argon",
  "version": "4.0.0",
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "description": "Google Marketing Platform Reporting to BigQuery connector",
  "homepage": "https://github.com/google/argon#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/argon.git"
  },
  "main": "./src/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "functions-framework --target=argon",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./dist/ --exec 'cd ./dist/ && npm run start'\"",
    "lint": "gts lint",
    "format": "gts fix",
    "clean": "gts clean",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "echo 'Error: No tests defined' && exit 1"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.10.0",
    "@google-cloud/functions-framework": "^2.1.1",
    "google-auth-library": "^7.12.0",
    "got": "^11.8.5",
    "split2": "^3.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.17",
    "@types/split2": "^3.2.1",
    "concurrently": "^7.0.0",
    "gts": "^3.1.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
